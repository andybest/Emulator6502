  processor 6502
  org $300
  
  jmp init

  include "uart.dasm"

init:  
  ldx     #$FF                 ; Initialize stack pointer to $01FF
  txs
  cld                          ; Clear decimal mode

  uarta_puts helloText
  uarta_puts testText

done:
  jmp done
        
helloText:  .byte "Hello, world", $0A, $0
testText:   .byte "This is a test of the Grif65 computer.", $0A, $0

